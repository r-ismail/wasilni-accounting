{
  "version": 2,
  "functions": {
    "src/main.ts": {
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "../../packages/shared/src/**"
    }
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "src/main.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/health",
      "dest": "src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
